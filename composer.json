{
    "name": "dianaphp/database",
    "description": "the diana database package",
    "keywords": [
        "diana",
        "php",
        "framework"
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Diana\\": "src/Diana/",
            "Diana\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "phpunit"
        ],
        "post-update-cmd": [
            "phpunit"
        ],
        "test": [
            "phpunit"
        ]
    },
    "authors": [
        {
            "name": "Antonio Ianzano",
            "email": "ianzanoan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "dianaphp/lib": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    }
}
